{
  "name": "auto-pr",
  "private": true,
  "description": "A pull request generator that uses coding agents to create and propose code changes.",
  "repository": "github:exKAZUu/auto-pr",
  "license": "UNLICENSED",
  "type": "module",
  "module": "src/cli.ts",
  "scripts": {
    "build": "build-ts app --input src/cli.ts",
    "check-all-for-ai": "bun run check-for-ai && bun run test",
    "check-for-ai": "bun install > /dev/null && bun run cleanup && bun run typecheck",
    "cleanup": "bun --bun wb lint --fix --format",
    "dev": "bun run src/cli.ts",
    "format": "bun --bun wb lint --format",
    "lint": "bun --bun wb lint",
    "lint-fix": "bun --bun wb lint --fix",
    "prepare": "lefthook install || true",
    "start": "bun run build && node dist/cli.js",
    "test": "bun test",
    "typecheck": "bun --bun wb typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/bun": "1.3.2",
    "@types/micromatch": "^4.0.10",
    "@willbooster/biome-config": "3.2.3",
    "@willbooster/prettier-config": "10.2.1",
    "@willbooster/wb": "10.6.13",
    "build-ts": "17.0.1",
    "lefthook": "2.0.2",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.7",
    "sort-package-json": "3.4.0",
    "typescript": "5.9.3"
  }
}
